const buildBreadcrumbSlider = function(){
    const plaSlider = document.querySelector('.pla-slider__wrapper');
    try{
        if(!!plaSlider) {
            let slider = plaSlider.querySelector('.pla-slider'),
            sliderTrack = plaSlider.querySelector('.pla-slider__track'),
            slides = plaSlider.querySelectorAll('.pla-slide'),
            controls = plaSlider.querySelector('.pla-controls');
        
            const buildPLASlider = () => {
                const slideCount = sliderTrack.querySelectorAll('.pla-slide').length;
                let group = 1;
                slides.forEach(function(slide,index){
                    if(index % 6 === 0 && index !== 0) {
                        group++;
                    }
                    slide.setAttribute('data-group', group);
                    slide.setAttribute('aria-label', `${index + 1} / ${slideCount}`);
                });
        
                if(slideCount <= 6) {
                    sliderTrack.classList.add('static-track');
                    controls.style.display = "none";
                }
        
                controlFunctionality(slideCount);
            };
        
            const controlFunctionality = (slideCount) => {
                let prevButton = controls.querySelector('.pla-prev'),
                    nextButton = controls.querySelector('.pla-next'),
                    slideDistance = 0,
                    groupCount = slideCount / 6,
                    groupCurrent = 1,
                    slides = sliderTrack.querySelectorAll('.pla-slide');
        
                function setActiveSlides(slides) {
                    slides.forEach(function(slide){
                        if(slide.querySelector('a')) {
                            slide.querySelectorAll('a').forEach(link => link.setAttribute('tabindex', '-1'));
                        }
                        
                        if(parseInt(slide.getAttribute('data-group')) === groupCurrent) {
                            if(slide.querySelector('a')) {
                                slide.querySelectorAll('a').forEach(link => link.setAttribute('tabindex', '0'));
                            }
                        }
                    });
                }
        
                nextButton.addEventListener('click', function(){
                    let sliderTrack = slider.querySelector('.pla-slider__track');
                    sliderTrack.setAttribute('data-direction', 'next');
                    groupCurrent++;
                    if(Math.ceil(groupCount) === groupCurrent) {
                        this.classList.remove('active-toggle');
                        switch(slideCount % 6) {
                            case 1:
                                slideDistance += 16.666;
                                break;
                            case 2:
                                slideDistance += 33.333;
                                break;
                            case 3:
                                slideDistance += 50;
                                break;
                            case 4:
                                slideDistance += 66.677;
                                break;
                            case 5:
                                slideDistance += 83.333;
                                break;
                            default: 
                                slideDistance += 100;
                                break;
                        }
                    } else {
                        slideDistance += 100;
                    }
        
                    sliderTrack.style.transform = `translateX(-${slideDistance}%)`;
                    
                    prevButton.classList.add('active-toggle');
                    
                    setActiveSlides(slides);
                });
        
                prevButton.addEventListener('click', function(){
                    let sliderTrack = slider.querySelector('.pla-slider__track');
                    sliderTrack.setAttribute('data-direction', 'prev');
                    if(groupCount < groupCurrent) {
                        switch(slideCount % 6) {
                            case 1:
                                slideDistance -= 16.666;
                                break;
                            case 2:
                                slideDistance -= 33.333;
                                break;
                            case 3:
                                slideDistance -= 50;
                                break;
                            case 4:
                                slideDistance -= 66.677;
                                break;
                            case 5:
                                slideDistance -= 83.333;
                                break;
                            default: 
                                slideDistance -= 0;
                                break;
                        }
                    } else {
                        slideDistance -= 100;
                    }
                    groupCurrent--;
                    
                    if(groupCurrent === 1) {
                        this.classList.remove('active-toggle');
                    } 
                    
                    sliderTrack.style.transform = `translateX(-${slideDistance}%)`;
                    nextButton.classList.add('active-toggle');
                    
                    setActiveSlides(slides);
                });
            };
        
            buildPLASlider();
        }
    } catch(error) {
        console.log('ERROR NLA PDP TEMPLATE -- PLACEMENT BELOW BREADCRUMB -- INIT CAROUSEL FUNCTIONALITY', error);
    }
};

